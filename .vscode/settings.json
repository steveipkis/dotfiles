{
    "workbench.colorTheme": "GitHub Dark",
    "window.zoomLevel": 1,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorSurroundingLines": 8,
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "trailing",
    "editor.snippetSuggestions": "top",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 500,
    "files.trimTrailingWhitespace": true,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/.direnv/**": true,
        "**/.venv/**": true,
        "**/venv/**": true,
        "**/.ipynb_checkpoints": true,
        "**/.pytest_cache": true,
        "**/target": true,
        "**/.bloop": true,
        "**/.metals": true,
        "**/.ammonite": true
    },
    "search.exclude": {
        "**/.ipynb_checkpoints": true,
        "**/.pytest_cache": true,
        "**/node_modules": true,
        "**/bower_components": true,
        "**/venv/**": true,
        "**/.venv/**": true,
        "**/.git/**": true,
        "**/project": true,
        "**/target": true,
        "**/.bloop": true,
        "**/.metals": true
    },
    "python.defaultInterpreterPath": ".venv/bin/python",
    "python.languageServer": "Default",
    "pythonIndent.trimLinesWithOnlyWhitespace": true,
    "vim.enableNeovim": true,
    "vim.neovimPath": "/Users/hnadeem/Applications/homebrew/bin/nvim",
    "vim.neovimUseConfigFile": false,
    "vim.neovimConfigPath": "/Users/hnadeem/.config/nvim/init.vim",
    "vim.leader": " ",
    "vim.commandLineModeKeyBindings": [],
    "vim.easymotion": true,
    "vim.sneak": false,
    "vim.replaceWithRegister": true,
    "vim.smartRelativeLine": false,
    "vim.useSystemClipboard": true,
    "vim.visualstar": true,
    "vim.hlsearch": true,
    "vim.shell": "bin/zsh",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<tab>"
            ],
            "commands": [
                ":bnext"
            ]
        },
        {
            "before": [
                "<s-tab>"
            ],
            "commands": [
                ":bprevious"
            ]
        },
        {
            "before": [
                "x"
            ],
            "after": [
                "\"_",
                "x"
            ]
        },
        {
            "before": [
                "X"
            ],
            "after": [
                "\"_",
                "X"
            ]
        },
        {
            "before": [
                "0"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "^"
            ],
            "after": [
                "0"
            ]
        }
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<Esc>"
            ],
            "commands": [
                ":nohl"
            ]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "f"
            ],
            "commands": [
                "editor.action.formatSelection"
            ]
        },
        {
            "before": [
                "<"
            ],
            "after": [
                "<",
                "g",
                "v"
            ]
        },
        {
            "before": [
                ">"
            ],
            "after": [
                ">",
                "g",
                "v"
            ]
        }
    ],
    "git.openRepositoryInParentFolders": "never",
    "workbench.iconTheme": "vscode-great-icons",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "eslint.codeActionsOnSave.rules": null,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "terminal.explorerKind": "external",
    "terminal.external.osxExec": "iterm2",
    "workbench.startupEditor": "none",
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    }
}
